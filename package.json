{
  "name": "subflow",
  "version": "0.9.5",
  "description": "Safety your data flow in TypeScript with flow-based primitives.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    }
  },
  "scripts": {
    "build": "vite build --config vite.build.config.ts && tsc --emitDeclarationOnly --outDir dist",
    "build:watch": "vite build --watch",
    "dev": "vite",
    "test": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "usecase": "vite-node --watch usecases/index.ts",
    "publish:core": "npm publish --access public"
  },
  "keywords": [
    "typescript",
    "functional",
    "monad",
    "data-flow",
    "safe",
    "immutable",
    "subflow"
  ],
  "files": [
    "dist"
  ],
  "author": "GrangbelrLurain",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GrangbelrLurain/subflow.git"
  },
  "homepage": "https://github.com/GrangbelrLurain/subflow",
  "packageManager": "pnpm@10.6.2",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "3.1.1",
    "@vitest/ui": "^3.1.1",
    "chokidar": "^4.0.3",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.1",
    "vite-node": "^3.1.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  }
}
